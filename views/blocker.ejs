<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link href="/stylesheets/style.css" rel="stylesheet" />

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</head>
  <script>
    $(function(){
      $('[data-toggle="tooltip"]').tooltip();

      $('.btn--event-type').click(function(){
        $('.btn--event-type').removeClass('btn--box__selected');
        $(this).addClass('btn--box__selected');

        $('.modal__btn#update').removeClass('btn--error').text($('.modal__btn#update').attr('data-default-text'));
      });

      $('.btn--selector').click(function(){
        $(this).toggleClass('btn--box__selected');
      });

      $('.modal__btn#update').click(function(){
        if(!$('.btn--event-type.btn--box__selected').length){
          $(this).addClass('btn--error').text('Oops! Please select a primary event type to proceed.');

          return;
        }

        var height = $('.modal').height();

        if(height + 64 > $(window).height()){
          $('.modal').animate({
            height: '450px'
          }, 450, 'easeInOutCubic');
        }

        $('.modal__success').show().animate({
          backgroundColor: '#57D396',
          height: '100%'
        }, 450, 'easeInOutCubic', function(){
          $('.modal__success__content').animate({
            opacity: 1
          });
        });
      });

      $('.modal__btn#close').click(function(){
        $('.modal').animate({
          top: '150px',
          position: 'relative',
          opacity: 0
        }, 450, 'easeInOutCubic');
      });
    });
  </script>

  <body>
  <!-- <body class="descriptions"> -->

    <div class="background"></div>

    <div class="modal">

      <div class="modal__content">
        <h1>Want to achieve a higher ranking? Update your listing!</h1>
